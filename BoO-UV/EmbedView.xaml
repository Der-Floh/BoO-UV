<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             BackgroundColor="AntiqueWhite"
             x:Class="BoO_UV.EmbedView">
    <Grid x:Name="objectGrid" ColumnSpacing="30">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="10*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <Button x:Name="Cancel" Text="Cancel" Grid.Row="0" Grid.Column="2" Clicked="OnCancelButtonClicked"/>
        <Grid x:Name="ObjectContent" Grid.Row="1" Grid.Column="1" BackgroundColor="Beige"/>
    </Grid>
</ContentView>
